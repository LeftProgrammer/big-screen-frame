{
  "name": "@jinghe/jinghe-lanhai",
  "version": "1.0.0",
  "description": "A Vue 3 based big screen framework",
  "author": "jinghe",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "types",
    "README.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./types/core/index.d.ts",
      "require": "./dist/core.cjs",
      "import": "./dist/core.js"
    },
    "./components": {
      "types": "./types/components/index.d.ts",
      "require": "./dist/components.cjs",
      "import": "./dist/components.js"
    },
    "./application": {
      "types": "./types/application/index.d.ts",
      "require": "./dist/application.cjs",
      "import": "./dist/application.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview --port 4173",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.vue",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist types",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run build:types"
  },
  "peerDependencies": {
    "vue": "^3.4.21",
    "pinia": "^2.1.7"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "echarts": "^5.5.0",
    "element-plus": "^2.6.2",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^24.0.0",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.5",
    "sass": "^1.72.0",
    "typescript": "~5.4.3",
    "vite": "^5.2.6",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.7"
  },
  "keywords": [
    "vue3",
    "typescript",
    "big-screen",
    "dashboard",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo/jinghe-lanhai.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.css"
  ]
}